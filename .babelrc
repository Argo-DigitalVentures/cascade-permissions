{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "node": "6.10"
        }
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [["env"]]
    }
  },
  "plugins": ["transform-es2015-modules-commonjs", "transform-es2017-object-entries", "transform-object-rest-spread"]
}
